
public class TwoSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int arr[]= {11,3,47,9,14,2};
		int target=17;
		int ans[]= new int[2];
		
		for(int i=0;i<arr.length;i++) {
			for(int j=i+1;j<arr.length;j++) {
			int s=arr[i]+arr[j];
			if(s==target)
				{
					ans[0]=i;
					ans[1]=j;
					break;
				}
			}
		}
		System.out.println(ans.toString());
	}

}
